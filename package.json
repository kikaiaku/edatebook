{
  "name": "calendar_app",
  "version": "1.0.0",
  "main": "server.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.20.0",
    "bcryptjs": "2.4.3",
    "bootstrap": "^4.5.2",
    "dotenv": "^8.2.0",
    "exceljs": "^4.1.1",
    "express": "^4.17.1",
    "express-session": "^1.16.1",
    "js-file-download": "^0.4.12",
    "json2xls": "^0.1.2",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
